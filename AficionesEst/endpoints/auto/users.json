{
    "getAll": {
        "description": "Gets all entries from 'users'",
        "route": "/users",
        "method": "GET",
        "sql": "SELECT * FROM users",
        "auth_required": false,
        "allowed_roles": [
            "*"
        ]
    },
    "getById": {
        "description": "Gets an entry from 'users' by its primary key",
        "route": "/users/$user_id",
        "method": "GET",
        "sql": "SELECT * FROM users WHERE user_id = $user_id",
        "auth_required": false,
        "allowed_roles": [
            "*"
        ]
    },
    "create": {
        "description": "Creates a new entry in 'users'",
        "route": "/users",
        "method": "POST",
        "sql": "INSERT INTO users (full_name, gender, age, email) VALUES ($full_name, $gender, $age, $email)",
        "request_body_params": [
            "full_name",
            "gender",
            "age",
            "email"
        ],
        "auth_required": true,
        "allowed_roles": [
            "*"
        ]
    },
    "update": {
        "description": "Updates an existing entry in 'users' by its primary key",
        "route": "/users/$user_id",
        "method": "PUT",
        "sql": "UPDATE users SET full_name = $full_name, gender = $gender, age = $age, email = $email WHERE user_id = $user_id",
        "request_body_params": [
            "full_name",
            "gender",
            "age",
            "email"
        ],
        "auth_required": true,
        "allowed_roles": [
            "*"
        ]
    },
    "delete": {
        "description": "Deletes an existing entry in 'users' by its primary key",
        "route": "/users/$user_id",
        "method": "DELETE",
        "sql": "DELETE FROM users WHERE user_id = $user_id",
        "auth_required": true,
        "allowed_roles": [
            "*"
        ]
    }
}