@startuml aficiones-est-dc

Enum Género << Enumerado >>{
    Masculino
    Femenino
    Otros
} 
    Class Usuario << Entidad >>{
        nombre: String
        género: Género
        edad: Integer
        email: String
    } 
    Class Afición {
        afición: TipoAfición
    } 
    Enum TipoAfición {
        Literatura 
        Cine
        Deporte 
        Gastronomía
    } 

    Usuario *-- "*" Afición
@enduml
